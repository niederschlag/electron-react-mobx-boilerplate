{
  "name": "electron-react-mobx-boilerplate",
  "version": "0.0.1",
  "description": "electron-react-mobx-boilerplate",
  "main": "init.js",
  "author": {
    "name": "niederschlag",
    "email": "hello@krkn.co"
  },
  "repository": "https://github.com/jschr/electron-react-redux-boilerplate",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.22.0",
    "history": "^4.6.3",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.2",
    "react-router": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^4.0.0",
    "redux-actions": "^2.2.1",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.23.6",
    "chai": "^4.1.0",
    "concurrently": "^3.5.1",
    "electron": "^1.8.4",
    "electron-builder": "^20.0.7",
    "electron-devtools-installer": "^2.1.0",
    "electron-mocha": "^6.0.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.1.0",
    "flow-runtime": "^0.17.0",
    "npm-run-all": "^4.0.1",
    "redux-mock-store": "^1.2.2",
    "rimraf": "^2.5.4"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "dev": "yarn private:compile --source-maps true && run-p -r private:watch private:serve",
    "test": "electron-mocha --renderer -R spec --require babel-core/register test/**/*.spec.js",
    "lint": "eslint --no-ignore scripts app test *.js",
    "package": "run-s private:clean private:compile private:build:all",
    "package:mac": "run-s private:clean private:compile private:build:mac",
    "package:win": "run-s private:clean private:compile private:build:win",
    "package:linux": "run-s private:clean private:compile private:build:linux",
    "private:build:all": "electron-builder -mwl",
    "private:build:mac": "electron-builder --mac",
    "private:build:win": "electron-builder --win",
    "private:build:linux": "electron-builder --linux",
    "private:watch": "yarn private:compile --source-maps true --watch --skip-initial-build",
    "private:serve": "babel-node scripts/serve.js",
    "private:compile": "babel app/ --copy-files --out-dir build",
    "private:clean": "rimraf build"
  }
}
