{
  "name": "electron-react-mobx-boilerplate",
  "description": "starter for create desktop application with electron & react & Mobx ",
  "version": "1.0.0",
  "author": {
    "name": "niederschlag",
    "email": "hello@krkn.co"
  },
  "scripts": {
    "dev": "concurrently -k \"yarn dev:serve\" \"yarn dev:electron\"",
    "dev:serve": "cross-env NODE_ENV=development webpack-dev-server",
    "dev:electron": "cross-env NODE_ENV=development electron index",
    "build": "yarn private:clean && yarn build:dist && yarn build:all",
    "build:dist": "cross-env NODE_ENV=production webpack --config webpack.production.config.js",
    "build:all": "build -mwl",
    "private:clean": "yarn rimraf build"
  },
  "keywords": [
    "electron",
    "react",
    "mobx",
    "boilerplate"
  ],
  "license": "MIT",
  "dependencies": {
    "history": "^4.6.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "electron": "^1.7.11",
    "electron-builder": "^19.55.2",
    "html-webpack-plugin": "^2.30.1",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^2.11.1"
  }
}
